<%= form_for @email, url:{action:"email_subscribe_post", :controller=>"email_subscribes"} do |f| %>
	<div>
	    <%= f.label :email %><br />
	    <%= f.email_field :email, autocomplete: "email", class: "form-control" %>
	</div>
	<div>
		<%= f.submit "Subscribe", class: "btn btn-success" %>
	</div>
<% end %>